{
  "name": "kodex",
  "type": "module",
  "version": "2.1.0",
  "private": true,
  "description": "A modern AI-powered documentation platform with integrated Git-like version control",
  "author": "KODEX Team",
  "license": "MIT",
  "homepage": "https://kodex.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/kodex.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/kodex/issues"
  },
  "keywords": [
    "documentation",
    "version-control",
    "markdown",
    "ai-generation",
    "astro",
    "react",
    "typescript",
    "kodex"
  ],
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src --ext .ts,.tsx,.astro",
    "lint:fix": "eslint src --ext .ts,.tsx,.astro --fix",
    "type-check": "astro check",
    "format": "prettier --write src/**/*.{ts,tsx,astro,md}",
    "clean": "rm -rf dist .astro node_modules/.cache"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.0",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^6.0.0",
    "@headlessui/react": "^2.2.4",
    "@supabase/supabase-js": "^2.39.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^5.2.5",
    "clsx": "^2.1.1",
    "highlight.js": "^11.9.0",
    "lucide-react": "^0.263.1",
    "marked": "^12.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.16"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}